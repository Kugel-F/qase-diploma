<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="SmokeAPI" parallel="tests" thread-count="4">

    <test name="ProjectTest">
        <classes>
            <class name="tests.api.ProjectTest"/>
        </classes>
    </test>
    <test name="CasesTest">
        <classes>
            <class name="tests.api.CasesTest"/>
        </classes>
    </test>
    <test name="SuiteTest">
        <classes>
            <class name="tests.api.SuiteTest"/>
        </classes>
    </test>
    <test name="RunTest">
        <classes>
            <class name="tests.api.RunTest"/>
        </classes>
    </test>
</suite>
